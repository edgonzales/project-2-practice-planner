<%- include('../partials/header') %>

<body>
    <h1>All Practices!</h1>

    <tbody>
        <% practiceDocs.forEach(practice => { %>
            <tr>
                <td>
                    <%= practice.name %> <br>
                </td>
                <td>
                    <%= practice.date %> <br>
                </td>
                <% practice.drills.forEach(drill => { %>
                 <%= drill.name %> <br>
                <% }) %>
                <td>
                    <a href="/practices/<%= practice._id %>">View Details</a>
                </td><br>
            </tr>
        <% }) %>
    </tbody>


    <a href="/practices/new/">Add Practice</a>

</body>
</html>